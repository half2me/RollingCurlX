{
  "name": "half2me/curlx",
  "type": "library",
  "description": "Wrapper for cUel Multi",
  "keywords": ["curl","multi","thread", "async"],
  "homepage": "https://github.com/half2me/curlx",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "CurlX\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CurlX\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "dev-master",
    "phpunit/phpunit": "~5.2"
  },
  "support": {
    "source": "https://github.com/half2me/curlx"
  },
  "extra": {
    "installer-name": "CurlX"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Benjamin Tamasi",
